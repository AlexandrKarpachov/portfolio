<hibernate-mapping>
	<class name="ru.job4j.todo.models.Item" table="items">
		<id name="id" column="id">
			<generator class="identity"/>
		</id>	
		<property name="desc" column="description"/>
		<property name="date" column="created"/>
		<property name="done" column="done"/>
	</class>
</hibernate-mapping>